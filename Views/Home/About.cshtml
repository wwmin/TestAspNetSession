@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="row">
    <div class="col-md-12">
        <h2>OldSessionId : @ViewBag.OldSessionId</h2>
        <h2>NewSessionId : @ViewBag.NewSessionId</h2>
        <h2>SessionName : @HttpContext.Current.Session["name"]</h2>
    </div>
</div>